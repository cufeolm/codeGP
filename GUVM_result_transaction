class GUVM_result_transaction extends uvm_transaction;

   logic[31:0] output_data;
   logic[31:0] target_address;   // check the target address after branch inst
   bit status;                   // for compare instr in amber

   function new(string name = "");
      super.new(name);
   endfunction : new

function string convert2string();
      string s;
      s = $sformatf("output_data: %4h",output_data);
      return s;
   endfunction : convert2string

endclass : GUVM_result_transaction
